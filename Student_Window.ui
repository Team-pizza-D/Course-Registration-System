      <property name="styleSheet">
       <string notr="true">self.setStyleSheet
/* Scroll area background (content behind the cards) */
QScrollArea {
    background-color: #1e1e1e;
    border: none;
}

/* Horizontal scroll bar */
QScrollBar:horizontal {
    height: 10px;
    background: #181818;
    margin: 0px 24px 0px 24px;      /* space before/after arrows */
    border-radius: 5px;
}

/* The draggable handle */
QScrollBar::handle:horizontal {
    background: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0 #3a3a3a,
        stop: 1 #595959
    );
    border-radius: 5px;
    min-width: 40px;
}

/* Handle hover &amp; pressed */
QScrollBar::handle:horizontal:hover {
    background: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0 #4a4a4a,
        stop: 1 #707070
    );
}

QScrollBar::handle:horizontal:pressed {
    background: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0 #606060,
        stop: 1 #808080
    );
}

/* Remove ugly arrows but keep margin clean */
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    background: none;
    width: 0px;
    height: 0px;
}

/* The area before and after the handle */
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}
